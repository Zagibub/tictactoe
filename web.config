<configuration>
    <system.webServer>
        <handlers>
            <!-- indicates that the app.js file is a node.js application to be handled by the iisnode module -->
            <add name="iisnode" path="server.js" verb="*" modules="iisnode" />
        </handlers>
         <staticContent>
            <mimeMap fileExtension=".json" mimeType="application/json" />
        </staticContent>
        </system.webServer>
</configuration>
